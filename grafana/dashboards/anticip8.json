        {
          "expr": "sum(rate(anticip8_cache_hits_total{namespace=\"options\"}[1m])) / clamp_min(sum(rate(anticip8_cache_hits_total{namespace=\"options\"}[1m])) + sum(rate(anticip8_cache_misses_total{namespace=\"options\"}[1m])), 1e-9)",
          "refId": "A"
        }
      ]
    },
    {
